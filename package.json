{
	"name": "auto-upload",
	"displayName": "auto-upload",
	"description": "复制文件路径后，粘贴到编辑器里会自动上传到七牛云并替换成远程连接",
	"version": "1.0.1",
	"publisher": "JayZangwill",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:auto-upload.upload"
	],
	"keywords": [
		"auto-upload",
		"uplaod",
		"qiniu",
		"auto",
		"clipboard"
	],
	"main": "./extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "请求接口的一些配置",
			"properties": {
				"autoUpload.hostname": {
					"type": "string",
					"default": "",
					"description": "请求接口的域名"
				},
				"autoUpload.port": {
					"type": "number",
					"default": 80,
					"description": "端口"
				},
				"autoUpload.path": {
					"type": "string",
					"default": "",
					"description": "请求接口的路径"
				},
				"autoUpload.method": {
					"type": "string",
					"default": "POST",
					"description": "请求接口的方法"
				},
				"autoUpload.protocol": {
					"type": "string",
					"default": "http",
					"description": "协议"
				}
			}
		},
		"commands": [
			{
				"command": "auto-upload.upload",
				"title": "upload your img",
				"category": "upload"
			}
		],
		"keybindings": [
			{
				"command": "auto-upload.upload",
				"key": "ctrl+alt+v",
				"mac": "cmd+alt+v",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.48.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/JayZangwill/vscode-auto-upload"
	},
	"dependencies": {
		"clipboardy": "^2.3.0",
		"qiniu": "^7.3.2"
	}
}
